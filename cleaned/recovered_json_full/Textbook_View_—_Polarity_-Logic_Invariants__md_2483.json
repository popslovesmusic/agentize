{ // \============================================================ // Θ INTERACTION TABLE — logical gates × invariants // \============================================================ tier\_id: "tier\_08", table\_id: "tier\_08\_polarity\_logic\_interactions", entries: \[ { id: "INT\_Θ\_AND\_CONSERVE", operators: \["Θ", "AND"\], invariant: "Θ\_INV\_CONSERVATION", effect: "AND must propagate polarity (cannot output neutral 0).", nf\_condition: "Θ(out) \= Θ(a) ⊕ Θ(b)", }, { id: "INT\_Θ\_OR\_CONSERVE", operators: \["Θ", "OR"\], invariant: "Θ\_INV\_CONSERVATION", effect: "OR cannot discard polarity on any branch.", nf\_condition: "Output sign must follow OR aggregation rule.", }, { id: "INT\_Θ\_NOT\_TENSOR\_SIGN", operators: \["Θ", "NOT"\], invariant: "Θ\_INV\_TENSOR\_SIGN", // NOT is allowed to flip sign, but ONLY explicitly effect: "NOT acts as explicit sign-flip operator.", nf\_condition: "Θ(out) \= \-Θ(in)", }, { id: "INT\_Θ\_PI\_BRIDGE\_TRUE", operators: \["Θ", "Π"\], invariant: "Θ\_INV\_TRUTH\_BRIDGE", effect: "Θ+ wires should evaluate to Π=true unless contradiction witness is present.", nf\_condition: "If Θ(x)='+', then Π(x)=true or witness\!=∅", }, { id: "INT\_Θ\_PI\_BRIDGE\_FALSE", operators: \["Θ", "Π"\], invariant: "Θ\_INV\_TRUTH\_BRIDGE", effect: "Θ- wires cannot evaluate to Π=true unless NOT/contradiction exists.", nf\_condition: "If Θ(x)='-', Π(x)=false unless negation present.", }, \], }