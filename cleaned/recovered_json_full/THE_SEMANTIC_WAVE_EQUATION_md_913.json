{ "delta": { "symbol": "δ", "definition": "semantic deviation operator", "signature": "δ : S → T\*S" }, "delta\_mu\_squared": { "symbol": "δ\_μ²", "definition": "μ-weighted deviation Laplacian", "formula": "δ\_μ² S \= μ⁻¹ ∇·( μ ∇S )" }, "phi": { "symbol": "Φ", "definition": "projection operator", "property": "Φ² \= Φ" }, "pi": { "symbol": "Π", "definition": "evaluation operator", "property": "Π² \= Π" }, "mu": { "symbol": "μ", "definition": "local adjacency weight", "type": "scalar\_weight\_field", "default\_value": 1.0 }, "lambda": { "symbol": "λ", "definition": "curvature generated by \[δ, Φ\]" }, "psi0": { "symbol": "ψ₀", "definition": "seed oscillation, ψ₀ \= \[δ, Π\]" }, "theta": { "symbol": "Θ", "definition": "polarity-interference operator, Θ \= \[Φ, Π\]" }, "chi": { "symbol": "χ", "definition": "semantic-time generator", "role": "d/dχ" }, "psi": { "symbol": "ψ", "definition": "ψS \= ω δS \+ η ΦS \+ κ ΠS", "components": \["ψω", "ψΦ", "ψΠ"\] } }